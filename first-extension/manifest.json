{
    "name": "Bookmark Checker",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "bookmarks",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches":["https://www.google.com.vn/search?*"],
            "js": ["content/content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action" : {
        "default_title" : "Bookmark checker",
        "default_icons": {
            "16": "/images/icon.png",
            "32": "/images/icon.png",
            "48": "/images/icon.png",
            "128": "/images/icon.png"
          }
      },
    "icons": {
        "16": "/images/icon.png",
        "32": "/images/icon.png",
        "48": "/images/icon.png",
        "128": "/images/icon.png"
      }
}